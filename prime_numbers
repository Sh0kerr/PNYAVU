#include <iostream>
#include <vector>
#include <cmath>


void prime(int n) {
    std::vector <int> primes;
    int num = 3;
    int counter = 1;
    bool flag = true;
    int sqr;
    while (counter < n) {
        sqr = ceil(sqrt(num));
        for (int prime: primes) {
            if (prime > sqr) break;
            if (num % prime == 0) {
                flag = false;
                break;
            }
        }
        if (flag) {
            primes.push_back(num);
            counter++;
            std::cout << num << ' ';
        }
        num += 2;
        flag = true;
    }
}


int main() {
	int n;
	std::cin >> n;
	std::cout << 2 << ' ';
	if (n > 1) prime(n);
	return 0;
}
