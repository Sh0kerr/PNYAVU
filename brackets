#include <iostream>
#include <string>
 
using namespace std;
 
 
string check(string line) {
    int n = line.length() - 1;
    string b = "";
    for (int i = 0; i < line.length() / 2; i++) {
            if ((line[i] != '(' and line[n - i] != ')') and (line[i] != '{' and line[n - i] != '}') and (line[i] != '[' and line[n - i] != ']')) {
                return "NO";
            }
            b = "";
        }
    return "YES";
}
 
 
int main(int argc, char *argv[])
{
    string line = "";
    string brackets = "";
    cin >> line;
    for (char symbol: line) {
        if (symbol == '(' or symbol == ')' or symbol == '{' or symbol == '}' or symbol == '[' or symbol == ']') {
            brackets += symbol;
        }
    }
    if (brackets.length() % 2 != 0) {
        cout << "NO";
    }
    else {
        cout << check(brackets);
    }
    return 0;
}
