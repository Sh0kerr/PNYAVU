#include <iostream>
#include <vector>


void prime(int n) {
    std::vector <int> primes;
    int num = 3;
    int counter = 1;
    int flag = 1;
    while (counter < n) {
        for (int prime: primes) {
            if (num % prime == 0) {
                flag = 1;
                break;
            }
        }
        if (flag == 0) {
                primes.push_back(num);
                counter++;
                std::cout << num << ' ';
        }
        num += 2;
        flag = 0;
    }
}


int main() {
	int n;
	std::cin >> n;
	std::cout << 2 << ' ';
	if (n > 1) prime(n);
	return 0;
}
