#include <iostream>
#include <string>
#include <stack>
#include <map>


std::string checker(std::string line) {
    std::stack<char> brackets;
    std::map<char, char> bracket_check = {
        {')', '('},
        {'}', '{'},
        {']', '['}
    };
    for (char symb: line) {
        if (symb == '(' || symb == '{' || symb == '[') {
            brackets.push(symb);
        }
        else if (symb == ')' || symb == '}' || symb == ']') {
            if (!brackets.empty() && brackets.top() == bracket_check[symb]) {
                brackets.pop();
            }
            else {
                return "NO";
            }
        }
    }
    return "YES";
}


int main() {
    std::string line;
    std::cin >> line;
    std::cout << checker(line);
}
