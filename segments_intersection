#include <iostream>
#include <vector>

using namespace std;

struct Dots
{
    float x[4];
    float y[4];
};

int main() {

    struct Dots dots;

    for(int i = 1; i < 5; i++){
        cin >> dots.x[i] >> dots.y[i];
    }

    if ((dots.x[4] - dots.x[3]) == 0 || (dots.x[2] - dots.x[1] == 0) || (((dots.y[2] - dots.y[1]) / (dots.x[2] - dots.x[1])) - ((dots.y[4] - dots.y[3]) / (dots.x[4] - dots.x[3]))) == 0) {
        return 0;
    }
    else {
        float x = (((dots.x[4] * dots.y[3] - dots.x[3] * dots.y[4]) / (dots.x[4] - dots.x[3]) - ((dots.x[2] * dots.y[1] - dots.x[1] * dots.y[2])) / (dots.x[2] - dots.x[1]))) / (((dots.y[2] - dots.y[1]) / (dots.x[2] - dots.x[1])) - ((dots.y[4] - dots.y[3]) / (dots.x[4] - dots.x[3])));
        float y = ((dots.y[4] - dots.y[3]) / (dots.x[4] - dots.x[3])) * x + ((dots.x[4] * dots.y[3] - dots.x[3] * dots.y[4]) / (dots.x[4] - dots.x[3]));
        if (x > min(dots.x[1], dots.x[2]) && x < max(dots.x[1], dots.x[2]) && x > min(dots.x[3], dots.x[4]) && x < max(dots.x[3], dots.x[4])) {
            cout << x << ' ' << y;
        }
        else return 0;
    }
}
